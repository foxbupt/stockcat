{
	"daily": {
        "queue": "daily-queue",
        "process_count": 1,
        "object": "daily_policy.DailyPolicy",
        "processor_list": [
            "serialize", "rise_factor", "day_trend"
        ]
	},
	"realtime": {
        "queue": "realtime-queue",
        "process_count": 1,
        "object": "realtime_policy.RTPolicy",
        "processor_list": [
            "serialize", "realtime_trend"
        ]
	},
	"chance": {
        "queue": "chance-queue",
        "process_count": 1,
        "object": "chance_policy.ChancePolicy",
        "loop_interval": 10,
        "loop_function": "rank",
        "processor_list": [
            "filter"
        ]
	}
} 
